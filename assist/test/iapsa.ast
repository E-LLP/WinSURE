SPACE    = ( NADS: 0..3,	(* NUMBER OF ACTIVE ADS SENSORS *)
      	     NFADS: 0..2,	(* NUMBER OF FAILED ADS SENSORS *)
   	     NMADS: 0..3);	(* NUMBER OF ACTIVE ADS MONITORS *)

DEATHIF	( NADS - NFADS < 1 ) OR ( NFADS > 1 )
   	OR ( NMADS < 2 AND NADS = 2 AND NFADS > 0 );

START    = (3, 0, 3);

LAMADS   = 40.0E-6;	(* ADS SENSOR FAILURE RATE *)
LAMMADS  = 5.0E-6;	(* ADS MONITOR CIRCUITRY FAILUTE RATE *)
LAMFIADS = 10.0E-6;	(* ADS FALSE ISOLATION RATE *)
REC1ADS  = 3.0E-4;	(* MEAN ADS SENSOR REC. TIME - FIRST FAULT *)
STD1ADS  = 1.2E-4;	(* SENSORY RECOVERY TIME STD DEV. -FIRST *)
REC2ADS  = 1.0E-4;	(* MEAN SENSOR REC. TIME - SECOND FAULT *)
STD2ADS  = 2.5E-5;	(* SENSOR RECOVERY TIME STD DEV -SECOND *)
COVADS   = 0.98;	(* ADS 2ND FAILURE COVERAGE *)

LAMC     = 10.0E-6;	(* I/S TERMINAL FAILURE RATE *)

   (* ADS FAILURE AND SIMUL. COMM FAILURE TRANSITIONS *)

IF NADS > 0 TRANTO (NADS, NFADS+1, NMADS) 
   BY (NADS - NFADS)*(LAMADS + NFADS*LAMC);


   (**** ADS FAILURE RECOVERY TRANSITIONS ****)

   	(* FIRST FAULT RECOVERY *)

IF (NADS > 2 AND NFADS > 0) THEN

   		(* LOSS OF MONITORED SENSOR *)
   IF NMADS > 1 TRANTO (NADS-1, NFADS-1, NMADS-1)
                BY < REC1ADS, STD1ADS, (NMADS/NADS) >;

   		(* LOSS OF UNMONITORED SENSOR *)
   IF (NADS > NMADS AND NMADS > 1) TRANTO (NADS-1, NFADS-1, NMADS)
                BY < REC1ADS, STD1ADS, (NADS - NMADS)/NADS >;

   		(* TWO SENSORS UNMONITORED *)
   IF NMADS < 2 TRANTO (NADS-1,NFADS-1,NMADS)
   		BY < REC1ADS, STD1ADS >;
   ENDIF;


   	(* SECOND FAULT RECOVERY *)

IF (NADS = 2 AND NMADS = 2 AND NFADS > 0) THEN
   TRANTO (NADS-1, NFADS-1,NMADS) BY < REC2ADS, STD2ADS, COVADS >;
   TRANTO (NADS-1, NFADS, NMADS) BY < REC2ADS, STD2ADS, 1.0 - COVADS >;
   ENDIF;


   	(* FALSE SENSOR ISOLATION AND BUS TERMINAL TRANSITION *)

IF (NADS > 2 AND NFADS < 1) THEN

   		(* LOSS OF MONITORED SENSOR *)
   IF NMADS > 1 TRANTO (NADS-1, NFADS, NMADS-1)
   		BY NMADS*(LAMFIADS/3.0 + LAMC);

   		(* LOSS OF UNMONITORED SENSOR *)
   IF NADS > NMADS AND NMADS > 1 TRANTO NADS = NADS-1
   		BY (NADS - NMADS) * (LAMFIADS/3.0 + LAMC);

   		(* TWO SENSORS UNMONITORED *)
   IF NMADS < 2 TRANTO NADS = NADS-1
   		BY NADS*(LAMFIADS/3.0 + LAMC);
   ENDIF;


   	(*** BUS TERMINAL FAULTS SUBSEQUENT TO FIRST FAULTS ***)

IF (NADS > 0 AND NADS < 3 AND NFADS < 1)
   TRANTO (NADS-1,NFADS,2) BY NADS * LAMC;

   	(* ADS MONITOR FAILURE TRANSITIONS *)

IF (NMADS > 1 AND NADS > 1 AND NFADS < 1)
   TRANTO NMADS = NMADS-1 BY NMADS*LAMMADS;

