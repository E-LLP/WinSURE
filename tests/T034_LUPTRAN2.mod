LAMBDA = 5E-5;
DELTA = 3600.0;
RHO = 1E5;
GAMMA = 10*LAMBDA;
PHI = DELTA/100 TO* 100*DELTA BY 10;
1,2= 3*LAMBDA;
2,3 = 2*LAMBDA + 2*GAMMA;
2,4 = < 1/DELTA, 1/DELTA >;
4,5 = LAMBDA + GAMMA;
1,6 =  3*GAMMA;
SIGMA = (* 1/(RHO+PHI); *) 1E-3;
6,1 = < 1/(RHO+PHI), SIGMA, RHO/(RHO+PHI) >;
6,4 = < 1/(RHO+PHI), SIGMA, PHI/(RHO+PHI) >;
6,7 = 2*LAMBDA + 2*GAMMA;
