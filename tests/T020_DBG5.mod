TIME = 1;
LBFACT = -1;
LAMBDA = 5E-4;
MU1 = 2.78E-4; SIG1 = 0;
MU2 = 1.39E-4; SIG2 = SQRT(6.43E-9);
1,2 = 3*LAMBDA;
2,3 = 2*LAMBDA;
2,4 = <MU1,SIG1>;
4,5 = 3*LAMBDA;
5,6 = 2*LAMBDA;
1,9 = LAMBDA;
2,9 = LAMBDA;
5,9 = <MU2,SIG2>;
LIST=2;
TMOM1 = (MU1*MU1+SIG1*SIG1);
TMOM2 = (MU2*MU2+SIG2*SIG2);
LBX = 2*LAMBDA*[MU1 - TMOM1*(3*LAMBDA/2 + 1/SQRT(MU1))];
LBY = 2*LAMBDA*[1 - 3*LAMBDA*MU1 - TMOM1/MU1]*
       [MU2 - TMOM2*LAMBDA - TMOM2/SQRT(MU2)];
ET1 = 0.75*[1 - EXP(-4*LAMBDA*TIME)];
TDEL = TIME - SQRT(MU1);
ET1DELTA = 0.75*[1 - EXP(-4*LAMBDA*(TDEL))];
ET2 = 0.75*[3.0*EXP(-4*LAMBDA*TIME)-3.0*EXP(-3*LAMBDA*TIME)+ 1];
TDEL = TIME-SQRT(MU1)-SQRT(MU2);
ET2DELTA = 0.75*[3.0*EXP(-4*LAMBDA*TDEL)-3.0*EXP(-3*LAMBDA*TDEL)+ 1];
UB1 = ET1*2*LAMBDA*MU1;
LB1 = LBX*ET1DELTA;
SHOW LB1,UB1;
UB2 = ET2*2*LAMBDA*MU2;
LB2 = ET2DELTA*LBY;
SHOW LB2,UB2;
