TIME = 1E-2 TO* 1E-1 BY 10;   (* %%%%%%%% CHANGED 7/20/99 %%%%%%%% *)
IR = 25*3600;   (* ITERATION RATE PER HOUR *)
LM = IR*1E-5;
L2 = IR*5E-7;
L3 = IR*5E-11;
L4 = IR*1E-14;
L5 = IR*1E-22;
P = .2;
RHO = 1.2E3;
1,2 = 4*LM;
1,3 = L2 + (1-P)*L3 + (1-P)*L4;
1,4 = P*L3 + P*L4 + (1-P)*L5;
1,5 = P*L5;
2,1 = RHO;
2,3 = 3*LM;
2,4 = L2 + (1-P)*L4;
2,5 = P*L4;
3,2 = RHO;
3,4 = 2*LM + L2;
3,5 = L4;
